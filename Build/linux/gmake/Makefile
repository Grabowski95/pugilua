# Type "make help" for usage help

ifndef config
  config=debug32
endif
export config

PROJECTS := pugilua

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

pugilua: 
	@echo "==== Building pugilua ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f pugilua.make

clean:
	@${MAKE} --no-print-directory -C . -f pugilua.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug32"
	@echo "   release32"
	@echo "   debug64"
	@echo "   release64"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   pugilua"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
